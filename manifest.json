{
	"manifest_version": 2,

	"name": "First Page Fiddle",
	"description": "Fiddles with student center",
	"version": "0.1",
	"permissions": ["activeTab", "https://css.adminapps.cornell.edu/*"],

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html",
		"default_title": "View Popup"
	},
  
	"background": {
		"scripts": ["jquery.js", "background.js"],
		"persistent": false
	}
}